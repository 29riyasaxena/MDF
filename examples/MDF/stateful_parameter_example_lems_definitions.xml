<?xml version="1.0" ?>
<Lems xmlns="http://www.neuroml.org/lems/0.7.6" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.neuroml.org/lems/0.7.6 https://raw.githubusercontent.com/LEMS/LEMS/development/Schemas/LEMS/LEMS_v0.7.6.xsd">
  <ComponentType extends="baseCellMembPotDL" name="sine_node__definition">
    <Parameter dimension="none" name="amp"/>
    <Parameter dimension="none" name="dt"/>
    <Attachments name="only_input_port" type="basePointCurrentDL"/>
    <Exposure dimension="none" name="update_time"/>
    <Exposure dimension="none" name="sin_function"/>
    <Exposure dimension="none" name="out_port"/>
    <Exposure dimension="none" name="only_output_port"/>
    <Dynamics>
      <DerivedVariable dimension="none" exposure="V" name="V" value="0"/>
      <DerivedVariable dimension="none" exposure="update_time" name="update_time" value="(time * 1 + dt)"/>
      <DerivedVariable dimension="none" exposure="sin_function" name="sin_function" value="amp * sin(time)"/>
      <DerivedVariable dimension="none" exposure="out_port" name="out_port" value="sin_function"/>
      <DerivedVariable dimension="none" exposure="only_output_port" name="only_output_port" value="out_port"/>
    </Dynamics>
  </ComponentType>
  <Component amp="3" dt="0.1" id="sine_node__instance" type="sine_node__definition"/>
</Lems>
